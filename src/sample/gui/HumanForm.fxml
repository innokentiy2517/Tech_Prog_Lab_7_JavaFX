<AnchorPane prefHeight="200.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.HumanFormController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefWidth="300.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name" GridPane.rowIndex="1" />
            <Label text="Age" GridPane.rowIndex="2" />
            <Label fx:id="moneyLabel" text="Label" GridPane.rowIndex="3" />
            <TextField fx:id="nameTxtField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="ageTxtField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="moneyTxtField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="courseLabel" text="Course" GridPane.rowIndex="4" />
            <ChoiceBox fx:id="humanTypeCmb" prefWidth="300.0" GridPane.columnSpan="2" />
            <ChoiceBox fx:id="courseCmb" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" onAction="#onSaveClick" text="Save" GridPane.rowIndex="5" />
            <Button mnemonicParsing="false" onAction="#onCancelClick" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="5">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
   </children>
</AnchorPane>
